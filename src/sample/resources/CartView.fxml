<fx:root prefHeight="683.0" prefWidth="332.0" style="-fx-background-color: background;" styleClass="default-primary-color" stylesheets="@palette.css" type="VBox" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label prefHeight="55.0" style="-fx-text-fill: white;" text="Kundvagn" VBox.vgrow="NEVER">
         <font>
            <Font size="19.0" />
         </font>
         <padding>
            <Insets left="8.0" />
         </padding>
      </Label>
      <ListView fx:id="cartList" prefHeight="0.0" prefWidth="332.0" VBox.vgrow="SOMETIMES">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" />
         </VBox.margin></ListView>
      <Button fx:id="buyButton" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="70.0" style="-fx-background-color: background; -fx-text-fill: white;" styleClass="accent-color" text="Kassa" VBox.vgrow="ALWAYS">
         <effect>
            <InnerShadow blurType="ONE_PASS_BOX" choke="1.0" color="WHITE" height="12.0" radius="5.5" width="12.0" />
         </effect>
         <font>
            <Font size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <graphic>
            <ImageView fitHeight="29.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@pay.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</fx:root>
